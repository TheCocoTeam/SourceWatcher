  "require": {
    "doctrine/dbal": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-pgsql": "*",
    "flow/jsonpath": "*",
    "guzzlehttp/guzzle": "^7.2",
    "monolog/monolog": "*",
    "php": "*",
    "php-snippets/circular-array": "^1.0",
